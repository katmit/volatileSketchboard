<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Volatile Sketchboard</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                @*style="display: table-cell;vertical-align: middle;"*@
                @*<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <img alt="thunderbolt" src="data:image/png;base64,">
                </button>*@
                @Html.ActionLink("Volatile Sketchboard", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li style="padding-top:8px;" >
                        <img alt="thunderbolt" width="32" height="32" src="data:image/png;base64,">
                    </li>
                    <li style="padding-left:20px;">@Html.ActionLink("Development", "About", "Home")</listyle="padding-top:8px;">
                </ul>
            </div>
        </div>
    </div>
    <div>
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/fabric")
    @RenderSection("scripts", required: false)
</body>
</html>
